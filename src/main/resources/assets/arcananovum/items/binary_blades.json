{
    "model": {
        "type": "minecraft:select",
        "property": "minecraft:custom_model_data",
        "cases": [
            {
                "when": "combined",
                "model": {
                    "type": "minecraft:model",
                    "model": "arcananovum:item/binary_blades_combined"
                }
            },
			{
                "when": "singular",
                "model": {
                    "type": "minecraft:model",
                    "model": "arcananovum:item/binary_blades_singular"
                }
            },
			{
                "when": "combined_pulsar",
                "model": {
                    "type": "minecraft:model",
                    "model": "arcananovum:item/binary_blades_combined_pulsar"
                }
            },
			{
                "when": "singular_pulsar",
                "model": {
                    "type": "minecraft:model",
                    "model": "arcananovum:item/binary_blades_singular_pulsar"
                }
            },
			{
                "when": "combined_white",
                "model": {
                    "type": "minecraft:model",
                    "model": "arcananovum:item/binary_blades_combined_white"
                }
            },
			{
                "when": "singular_white",
                "model": {
                    "type": "minecraft:model",
                    "model": "arcananovum:item/binary_blades_singular_white"
                }
            },
			{
                "when": "combined_red",
                "model": {
                    "type": "minecraft:model",
                    "model": "arcananovum:item/binary_blades_combined_red"
                }
            },
			{
                "when": "singular_red",
                "model": {
                    "type": "minecraft:model",
                    "model": "arcananovum:item/binary_blades_singular_red"
                }
            }
        ],
        "fallback": {
            "type": "minecraft:model",
            "model": "arcananovum:item/binary_blades_combined"
        }
    },
    "hand_animation_on_swap": false
}
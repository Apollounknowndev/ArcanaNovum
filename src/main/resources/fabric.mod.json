{
  "schemaVersion": 1,
  "id": "arcananovum",
  "version": "${version}",
  "name": "Arcana Novum",
  "description": "A Server-Side magic mod that adds various Magic Items to your server!",
  "authors": [
    "BorisShoes"
  ],
  "contact": {
    "repo": "https://github.com/borisshoes/ArcanaNovum"
  },
  "license": "MIT",
  "icon": "assets/arcananovum/icon.png",
  "environment": "server",
  "entrypoints": {
    "main": [
      "net.borisshoes.arcananovum.Arcananovum"
    ],
    "cardinal-components-entity": [
      "net.borisshoes.arcananovum.cardinalcomponents.PlayerComponentInitializer"
    ],
    "cardinal-components-world": [
      "net.borisshoes.arcananovum.cardinalcomponents.WorldDataComponentInitializer"
    ]
  },
  "custom": {
    "cardinal-components": ["arcananovum:profile","arcananovum:magicblocks","arcananovum:magicentities","arcananovum:login_callbacks"]
  },
  "mixins": [
    "arcananovum.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.8",
    "fabric": "*",
    "minecraft": "1.19"
  }
}
